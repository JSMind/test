# use_frameworks!
platform :ios, '8.0'
inhibit_all_warnings!

workspace ‘OMSSDKDemo.xcworkspace'

target ‘OMSSDKDemo' do
    pod 'AFNetworking'
    pod 'CATLog'
    pod 'YYModel'
    pod 'CocoaAsyncSocket'
    pod 'Reachability'
end

target ‘OMSSDK' do
    project ‘OMSSDK/OMSSDK.xcodeproj'
    pod 'AFNetworking'
    pod 'CATLog'
    pod 'YYModel'
    pod 'CocoaAsyncSocket'
    pod 'Reachability'
end
